  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.3",
    "vue-tsc": "^2.0.6"
  }
