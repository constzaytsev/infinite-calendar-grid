{
  "name": "infinite-calendar-grid",
  "description": "Infinite calendar grid component for Vue",
  "author": "Konstantin Zaytsev <zaytsev.const@gmail.com> (https://github.com/constzaytsev)",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/infinite-calendar-grid.umd.cjs",
  "module": "./dist/infinite-calendar-grid.js",
